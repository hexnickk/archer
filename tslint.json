{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "rxjs-tslint-rules"
    ],
    "jsRules": {},
    "rules": {
        "missing-jsdoc": false,
        "ordered-imports": false,
        "quotemark": [true, "single"],
        "object-literal-sort-keys": false,
        "no-empty": [true, "allow-empty-functions"]
    },
    "rulesDirectory": []
}